<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 
#ConfigSection
데이터베이스에 질의할 구문을 작성하는곳
 -->

<mapper namespace="jumps">
	<select id="selectJumpInfo" parameterType="map" resultType="map">
		SELECT TOP 1 * 
		FROM JumpOpenInfo
		WHERE jmp_db_name = #{jmp_db_name}			
			AND reg_id = #{reg_id}
	</select>
	<insert id="insertJumpInfo" parameterType="map">
		INSERT INTO JumpOpenInfo
			(j_param, frm_cd, jmp_db_name, reg_id)
			VALUES
			(#{j_param},#{frm_cd},#{jmp_db_name}, #{reg_id})		
	</insert>
	<update id="updateJumpInfo" parameterType="map">
		UPDATE A 
			SET A.j_param = #{j_param}
				,A.frm_cd = #{frm_cd}
		FROM JumpOpenInfo A
		WHERE A.jmp_db_name = #{jmp_db_name}	
			AND A.reg_id = #{reg_id}		
	</update>
	<delete id="deleteJumpInfo" parameterType="map">
		DELETE
		FROM JumpOpenInfo
		WHERE jmp_db_name = #{jmp_db_name}			
			AND A.reg_id = #{reg_id}
	</delete>
</mapper>


