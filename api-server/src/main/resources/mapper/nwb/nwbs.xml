<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 
#ConfigSection
데이터베이스에 질의할 구문을 작성하는곳
 -->

<mapper namespace="nwbs">
	<select id="nwbInfos" resultType="map">
		select * 
		from nwb
	</select>
	
	<select id="nwbInfoByNwbId" resultType="map">
		select *
		from nwb
		where
		nwb_id = #{nwb_id}
	</select>

</mapper>


